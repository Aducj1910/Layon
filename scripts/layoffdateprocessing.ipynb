        "import pandas as pd\n",
        "import datetime\n",
        "\n",
        "\n",
        "layoff = pd.read_csv(\"layoff.csv\")\n",
        "\n",
        "layoff['date'] = pd.to_datetime(layoff['date'])\n",
        "\n",
        "layoff['month'] = layoff['date'].dt.month_name()\n",
        "layoff['year'] = layoff['date'].dt.year\n",
        "\n",
        "layoff.to_csv('layofffinal.csv')"
      ]
    }
  ]
